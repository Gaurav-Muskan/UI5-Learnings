<mvc:View controllerName="restinpeace.project1.controller.Second"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="" title="{i18n>title}">
        <customHeader>
            <Bar>
                <contentMiddle>
                    <Title text="I am Second View" />
                </contentMiddle>
            </Bar>
        </customHeader>
        <content>
            <VBox>
                <HBox>
                    <Label text="Employee Id" required="true" width="150px"/>
                    <Text id="idIpEmpId" text="{myModel>empid}" />
                </HBox>
                <HBox>
                    <Label text="Employee Name" required="true" width="150px"/>
                    <Text id="idIpEmpName" text="{myModel>empname}" />
                </HBox>
                <HBox>
                    <Label text="Employee Designation" required="true" width="150px"/>
                    <Text id="idIpEmpDesignation" text="{myModel>empdesig}" />
                    <!-- WhatEver there is written as data member name in data.json that should be written here.Like empdesig here -->
                </HBox>
                <HBox>
                    <Label text="Employee Skill" required="true" width="150px"/>
                    <Text id="idIpEmpSkill" text="{myModel>empskill}" />
                </HBox>
                <HBox>
                    <Label text="Employee Email" required="true" width="150px"/>
                    <Text id="idIpEmpEmail" text="{myModel>empemail}" />
                </HBox>
                <HBox>
                    <Label text="Employee Location" required="true" width="150px"/>
                    <Text id="idIpEmpLocation" text="{myModel>emploc}" />
                </HBox>
            </VBox>
        </content>
        <footer>
            <Bar>
                <contentMiddle>
                    <Button text="Go to Third View" press="onGoThird" type="Emphasized" />
                    <Button text="Back" press="onGoFirst" type="Emphasized" />
                </contentMiddle>
            </Bar>
        </footer>
    </Page>
</mvc:View>
